{
    "name": "vs-script-commands",
    "displayName": "Script Commands",
    "description": "Adds additional commands to Visual Studio Code that uses scripts (JavaScript) for execution.",
    "version": "0.0.1",
    "publisher": "mkloubert",
    "engines": {
        "vscode": "^1.5.0"
    },
    "license": "MIT",
    "categories": [
        "Other"
    ],
    "keywords": [
        "Custom",
        "Commands",
        "Scripts",
        "JavaScript",
        "NodeJS"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": []
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "icon": "icon.png",
    "author": {
        "name": "Marcel Joachim Kloubert"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mkloubert/vs-script-commands"
    },
    "bugs": {
        "url": "https://github.com/mkloubert/vs-script-commands/issues"
    },
    "readmeFilename": "README.md"
}